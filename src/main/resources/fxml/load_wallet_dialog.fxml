<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane fx:id="dialogPane" headerText="Please, enter your wallet password." minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.byow.wallet.byow.gui.controllers.LoadWalletDialogController">
    <content>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="197.0" minHeight="0.0" prefHeight="27.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="230.0" minHeight="33.0" prefHeight="203.0" valignment="TOP" />
            </rowConstraints>
            <children>
                <Label text="Wallet Password" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <PasswordField fx:id="loadWalletPassword" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            </children>
        </GridPane>
    </content>
    <buttonTypes>
        <ButtonType fx:id="ok" fx:constant="OK" />
        <ButtonType fx:id="cancel" buttonData="CANCEL_CLOSE" text="Cancel" />
    </buttonTypes>
    <padding>
        <Insets left="10.0" right="10.0" top="10.0" />
    </padding>
</DialogPane>
